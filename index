<script>
    if (!String.prototype.includes) {
        String.prototype.includes = function() {
            'use strict';
            return String.prototype.indexOf.apply(this, arguments) !== -1;
        };
    }
    $( '.restrict-yet' ).keyup(function() {
        var str = $(this).val();
        var is_yet = str.includes('@');
        if(is_yet){
            $('#controlPanelLogin').prop('disabled', true);
            $(this).attr('data-content','Please use your domain name or username in this field');
            $(this).popover('show');
        }else{
            $('#controlPanelLogin').prop('disabled', false);
            $(this).attr('data-content','');
            $(this).popover('hide');
        }
    });
</script>

<script>
    window.document.addEventListener('DOMContentLoaded', function(event) {
        //for finding IE9
        var ua = window.navigator.userAgent;
        var msie = ua.indexOf('MSIE ');

        // Control-panel credential text validation
        $('#controlPanelLogin').click(function(e) {
            var cPanelLoginForm = $(this).closest("form");
            var username = cPanelLoginForm.find('input[name="credential_0"]').val();
            var password = cPanelLoginForm.find('input[name="credential_1"]').val();

            var usernamePattern = /^[a-z0-9-\.:]+$/i;
            var usernameRegex = new RegExp(usernamePattern);

            if (username.length == 0 ) {
                alert('Username cannot be empty');
                return false;
            }
            if (usernameRegex.test(username) == false) {
                alert('Invalid characters in username.');
                return false;
            }
            if (password.length == 0 ) {
                alert('Password cannot be empty');
                return false;
            }
        });


        //Hide the form and G Suite banner on the redirect
        var errorMessage = $('.error_msg').html();
        if(/Click here to continue/i.test(errorMessage)) {
            $('#login-content').hide();
        }

        //hide the error/success messages on changing the tabs
        $('#login-tabs a[data-toggle="tab"]').click(function() {
            $('.error').hide();
            $('.error_msg').hide();
            $('.success_msg').hide();
            $('.input-group').removeClass('invalid');
            $('#controlpanel .input-group').popover('destroy');
        });

        $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
            var target = $(e.target).attr('data-target') // activated tab

            if(parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10) !== 9){
                var url = window.location.search;
                var pageUrl = location.pathname;
                if(/showWebmailTab=1/i.test(url)) {
                    pageUrl = pageUrl + '?showWebmailTab=1';
                }
                window.history.pushState('', "iPage  Login", pageUrl + target);
            } else {
                window.location.hash = target;
                setTimeout(function() {
                    window.scrollTo(0, 0);
                }, 1);
            }

            if($('.gmail-for-work').length) {
                if (target == '#webmail') {
                    dataLayer.push({'pageType': 'flow', 'flow': 'webmail', 'step': 'login'});
                    $('.gmail-for-work').show();
                } else {
                    dataLayer.push({'pageClass': 'login', 'pageType': 'login'});
                    $('.gmail-for-work').hide();
                }
            }
        });

        $('.login-forms input').placeholder();
        $('#controlpanel .input-group').popover('show');

        var hash = window.location.hash;

        if(parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10) !== 9){
            var url = window.location.search;
            var pageUrl = location.pathname;

            if(/showWebmailTab=1/i.test(url)) {
                pageUrl = pageUrl + '?showWebmailTab=1';
            }
            window.history.pushState('', "iPage  Login", pageUrl);
        }

        if(hash) {
            $('#login-tabs a[data-target="' + hash + '"]').tab('show');
            setTimeout(function() {
                window.scrollTo(0, 0);
            }, 1);
        }

        $('.login-forms input.form-control').on('focus',function() {
            $('.input-group').removeClass('invalid');
            $('#controlpanel .input-group').popover('destroy');
        });

        $('.accordion-toggle').click(function () {
            $(this).find('span').toggleClass('fa-chevron-down fa-chevron-right');
        });
        $('#manage-mailbox').click(function(e) {
            e.preventDefault();
            var mailAdminForm = $(this).closest("form");
            mailAdminForm.attr('name', 'login');
            mailAdminForm.attr('action', '/mailLogin');
            mailAdminForm.find('input[name="destination"]').val('/mailadmin/index.bml?manage=1');
            mailAdminForm.find('#address').attr('name', 'credential_0');
            mailAdminForm.find('#password').attr('name', 'credential_1');
            mailAdminForm.submit();
        });
    });
</script>



            
