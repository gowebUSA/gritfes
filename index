<script type="text/javascript">
  (function(){
    var cookie_name = "Currency",
        currencyOptElems = document.getElementsByClassName('cur-opt');

    for(var i = 0; i < currencyOptElems.length; i++){
      currencyOptElems[i].addEventListener('click', function(e){
        var isDubdomain = [],
            currency = this.innerHTML;

        isDubdomain = location.hostname.split('.');

        if (! /^(USD|GBP|CAD|AUD|EUR)$/.test(currency)) {
          currency = 'USD';
        }

        if (isDubdomain.length == 2) {
          document.cookie=cookie_name+"="+currency+"; expires=Monday, 04-Apr-2020 05:00:00 GMT;  path=/; domain=."+location.hostname.split('.').splice(0,3).join('.');
        } else {
          document.cookie=cookie_name+"="+currency+"; expires=Monday, 04-Apr-2020 05:00:00 GMT;  path=/; domain=."+location.hostname.split('.').splice(1,4).join('.');
        }

        window.onbeforeunload=null;
        window.location.reload();

      }, false);
    }
  })();
</script>



